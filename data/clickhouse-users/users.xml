<yandex>
    <profiles>
        <default>
            <load_balancing>random</load_balancing>
            <use_uncompressed_cache>0</use_uncompressed_cache>
            <log_queries>1</log_queries>
            <log_queries_cut_to_length>100000</log_queries_cut_to_length>
            <max_memory_usage>10000000000</max_memory_usage>
            <max_threads>8</max_threads>
            <max_bytes_before_external_group_by>1000000000</max_bytes_before_external_group_by>
            <max_bytes_before_external_sort>1000000000</max_bytes_before_external_sort>
            <max_result_rows>100000</max_result_rows>
            <max_rows_in_distinct>100000</max_rows_in_distinct>
            <max_bytes_in_distinct>1000000000</max_bytes_in_distinct>
            <max_rows_in_set>100000</max_rows_in_set>
            <max_bytes_in_set>1000000000</max_bytes_in_set>
        </default>
    </profiles>
    <users>
        <default>
            <password>admin</password>
            <networks>
                <ip>::/0</ip>
            </networks>
            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>
    <quotas>
        <default>
            <interval>
                <duration>3600</duration>
                <queries>0</queries>
                <errors>0</errors>
                <read_rows>0</read_rows>
                <read_bytes>0</read_bytes>
                <execution_time>0</execution_time>
            </interval>
        </default>
    </quotas>
</yandex>